<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="MyStoreAutomation">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="base.CustomListeners" />
	</listeners>

	
	<test name="Login Test">
		<classes>
			<class name="testcases.TC2Login" />
		</classes>
	</test> <!-- Test -->
	
	<test name="Address Test">
		<classes>
			<class name="testcases.TC3AddAddress" />
		</classes>
	</test> <!-- Test -->
	
	<test name="Add To Cart Test">
		<classes>
			<class name="testcases.TC4AddCart" />
		</classes>
	</test> <!-- Test -->

</suite> <!-- Suite -->

